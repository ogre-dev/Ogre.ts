{
  "clearMocks": true,
  "coverageDirectory": "coverage",
  "coverageProvider": "v8",
  "coverageThreshold": {
    "global": {
      "branches": 50,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "preset": "ts-jest",
  "testEnvironment": "node",
  "testMatch": ["**/tests/*.test.ts"]
}
