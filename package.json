{
  "name": "@ogre.dev/framework",
  "version": "1.0.1",
  "description": "Ogres are like onions... They have layers; TypeScript framework for Node.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register index.ts",
    "lint": "eslint . --ext .js,.ts",
    "test": "jest --coverage --config jest.config.json",
    "documentation": "typedoc lib/index.ts",
    "clean": "gulp clean",
    "build": "gulp build",
    "package": "gulp package && cp LICENSE dist",
    "license": "gulp license",
    "prepare": "husky install"
  },
  "author": "Maxence Maire <framework@ogre.dev>",
  "license": "MIT",
  "keywords": [
    "web",
    "app",
    "http",
    "application",
    "framework",
    "rest",
    "restful",
    "typescript",
    "middleware",
    "layer"
  ],
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/gulp": "^4.0.8",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "del": "^6.0.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-tsdoc": "^0.2.14",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  }
}
